#!/usr/bin/env ruby

require "rubygems"
require "bundler/setup"

require 'logger'
require File.expand_path('../../lib/git-process', __FILE__)

gp = Git::Process.new('.', :log_level => Logger::INFO)

begin
  gp.rebase_to_master
rescue Git::Process::GitProcessError => exp
  puts exp.message
  exit -1
end
