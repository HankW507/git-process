#!/usr/bin/env ruby

require "rubygems"
require "bundler/setup"

require 'logger'
require 'backports'
require_relative '../lib/git-process/git-process'

gp = Git::Process.new('.', :log_level => Logger::INFO)

begin
  gp.sync_with_server
rescue Git::Process::GitProcessError => exp
  puts exp.message
  exit -1
end
